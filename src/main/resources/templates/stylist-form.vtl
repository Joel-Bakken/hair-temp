<!-- stylist-form.vtl -->
<h1>Add a new stylist to the database</h1>
<form action="/stylists-setup" method="post" >
  <div class="form-group">
    <label for="name">What is the new stylist's name?</label>
    <input class="form-control" id="name" name="name" type="text">
  </div>
  <button type="submit" class="btn btn-default">Add Stylist</button>
</form>

